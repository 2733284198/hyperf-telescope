{
    "name": "windawake/hyperf-telescope",
    "description": "An elegant debug assistant for the hyperf framework.",
    "keywords": [
        "swoole",
        "hyperf",
        "debugging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "fengchezhi",
            "email": "619146134@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.5",
        "hyperf/framework": "2.0.*",
        "hyperf/di": "2.0.*",
        "hyperf/view": "2.0.*",
        "hyperf/json-rpc": "2.0.*",
        "hyperf/rpc-server": "2.0.*",
        "hyperf/rpc-client": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Wind\\Telescope\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Wind\\Telescope\\ConfigProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}